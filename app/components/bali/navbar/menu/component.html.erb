<%= tag.div(**options) do %>
  <ul class="menu menu-horizontal px-1">
    <% start_items.each do |start_item| %>
      <li><%= start_item %></li>
    <% end %>
  </ul>
  <% if end_items.any? %>
    <ul class="menu menu-horizontal px-1">
      <% end_items.each do |end_item| %>
        <li><%= end_item %></li>
      <% end %>
    </ul>
  <% end %>
<% end %>
